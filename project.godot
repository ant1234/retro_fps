;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Retro_FPS"
run/main_scene="res://world.tscn"
config/features=PackedStringArray("4.3", "GL Compatibility")
config/icon="res://icon.svg"

[autoload]

Helper="*res://helper.gd"
SceneRouter="*res://scene_scripts/scene_router.gd"
GameState="*res://globals/GameState.gd"
DialogueManager="*res://addons/dialogue_manager/dialogue_manager.gd"
CustomDialogueManager="*res://scene_scripts/CustomDialogueManager.gd"

[dialogue_manager]

runtime/balloon_path="res://ui/Dialogue_Balloon/dialogue_balloon.tscn"

[display]

window/size/viewport_width=1600
window/size/viewport_height=1200
window/stretch/mode="canvas_items"
window/stretch/aspect="keep_height"

[editor_plugins]

enabled=PackedStringArray("res://addons/dialogue_manager/plugin.cfg")

[filesystem]

import/blender/enabled=false

[global_group]

player=""
navigation_mesh_source_group=""
fish=""

[gui]

theme/custom="res://ui/main_theme.tres"

[importer_defaults]

